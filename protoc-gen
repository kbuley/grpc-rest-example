#go get google.golang.org/protobuf@latest
#go get google.golang.org/protobuf/cmd/protoc-gen-go@latest
#go get google.golang.org/grpc/cmd/protoc-gen-go-grpc@latest
#go get google.golang.org/grpc@latest
#go get github.com/grpc-ecosystem/grpc-gateway/protoc-gen-grpc-gateway@latest
#go get github.com/grpc-ecosystem/grpc-gateway/protoc-gen-swagger@latest
rm -f pkg/api/v1/*
protoc api/proto/v1/*.proto -I api/proto/v1/ --go-grpc_out=pkg/api/v1 --go_out=pkg/api/v1 --grpc-gateway_out=logtostderr=true:pkg/api/v1 --swagger_out=logtostderr=true:api/swagger/v1

